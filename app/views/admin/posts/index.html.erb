<table class='table'>
	<thead>
		<tr>
			<th>ID</th>
			<th>Title</th>
			<th>Posted</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |post| %>
		<tr>
			<td><%= post.id %></td>
			<td><%= link_to post.title, edit_admin_post_path(post) %></td>
			<td><%= post.created_at %></td>
		</tr>
		<% end %>
	</tbody>
</table>
